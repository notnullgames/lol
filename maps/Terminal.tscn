script/source = "extends CanvasLayer

var line = 0
var position = 0
var opmode = false

# show values + position of cursor
func drawIndicators():
	# TODO
	pass

# Called when the node enters the scene tree for the first time.
func _ready():
	drawIndicators()

func _input(event):
	if event.is_action_pressed('ui_accept'):
		opmode = not opmode
	if event.is_action_pressed('ui_up'):
		if opmode:
			# TODO
			pass
		else:
			line = (line + 1) % 32
	if event.is_action_pressed('ui_up'):
		if opmode:
			# TODO
			pass
		else:
			line = (line - 1) % 32
	if event.is_action_pressed('ui_left'):
		if not opmode:
			position = (position - 1) % 5
	if event.is_action_pressed('ui_right'):
		if not opmode:
			position = (position + 1) % 5
	drawIndicators()
"

